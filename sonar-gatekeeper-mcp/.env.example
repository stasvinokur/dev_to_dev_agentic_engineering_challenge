# SonarQube connection
SONAR_URL=http://localhost:9000
# SONAR_TOKEN генерируется автоматически при первом запуске.
# Если хотите указать вручную — раскомментируйте:
# SONAR_TOKEN=your_sonarqube_token_here
SONAR_PROJECT_KEY=sonar-gatekeeper-mcp

# Ollama LLM
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=qwen2.5-coder:1.5b
# Для продвинутого использования (нужен доступ к cloud-модели):
# OLLAMA_MODEL=qwen3-coder:480b-cloud

# Langfuse observability (self-hosted, преднастроено в docker-compose)
LANGFUSE_HOST=http://localhost:3000
LANGFUSE_PUBLIC_KEY=pk-lf-sonar-gatekeeper
LANGFUSE_SECRET_KEY=sk-lf-sonar-gatekeeper
